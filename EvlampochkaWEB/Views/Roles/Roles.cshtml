@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
               Id
            </th>
            <th>
               Name
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var role in Model) {
        <tr>
            <td>
                 @role.Id
            </td>
            <td>
                @role.Name
            </td>           
        </tr>
}
    </tbody>
</table>
